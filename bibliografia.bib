@article{gomez_campos_mecanismos_2010,
	title = {Mecanismos implicados en la fatiga aguda},
	volume = {10},
	issn = {1577-0354},
 howpublished = {\url{https://dialnet.unirioja.es/servlet/articulo?codigo=3634772}},
	abstract = {The muscular overtiredness can be defined as the incapacity to raise a level of the muscular strength or a determined intensity of exercise in a situation where athletes experiment efforts. According to the multifactor design of the overtiredness, the mechanisms involved in such a weakened state still remain unclear. With regard to a functional standpoint, this research is dedicated to consider both the central tiredness (nervous system) and the peripheral tiredness (neuromuscular system). The purpose of this study was to review the literature dedicated to the mechanisms involved in the explanation of the overtiredness with regard to various variables needed to be considered.
La fatiga muscular aguda, puede ser definida como la incapacidad para seguir generando un nivel de fuerza o una intensidad de ejercicio determinada, siendo una situación que se vive, se siente y que atletas o no experimentan; pero debido a su carácter multifactorial, aún los mecanismos de su formación permanecen imprecisos. Se considera la existencia de factores que afectan a nivel muscular, generando la fatiga neuromuscular, y factores que afectan al cerebro, generando la fatiga central. Por lo que el objetivo del presente estudio fue una revisión de literatura, sobre los mecanismos implicados en fatiga aguda, con el propósito de mostrar las diversas vías relacionadas con el proceso de fatiga y dirigir los caminos para su identificación.},
	language = {spa},
	number = {40},
	journal = {Revista Internacional de Medicina y Ciencias de la Actividad Física y del Deporte},
	author = {Gómez Campos, Rossana and Cossio Bolaños, Marco Antonio and Brousett Minaya, M. and Hochmuller Fogaca, R. T.},
	year = {2010},
	note = {Publisher: Comunidad virtual Ciencias del Deporte
Section: Revista Internacional de Medicina y Ciencias de la Actividad Física y del Deporte},
	pages = {3--18},
	file = {Snapshot:C\:\\Users\\34644\\Zotero\\storage\\5AKSUXUW\\articulo.html:text/html},
}
