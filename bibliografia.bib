@article{Articulo_din,
	title = {Uso del dinamómetro para mejorar la fuerza de la mano del adulto mayor: {Revisión} bibliográfica},
	volume = {5},
	issn = {2806-5794},
	shorttitle = {Uso del dinamómetro para mejorar la fuerza de la mano del adulto mayor},
         howpublished ={\url{https://editorialalema.org/index.php/pentaciencias/article/view/862}},
	doi = {10.59169/pentaciencias.v5i6.862},
	abstract = {En el adulto mayor existen diferentes tipos de enfermedades degenerativas que ponen en riesgo la salud física de los mismos, es por esa razón que se han creado diversas formas de tratamientos y terapias de intervención que ayudan a mejorar la calidad de vida de las personas, con instrumentos de fácil manejo en la población. Esta revisión tiene como objetivo determinar los efectos del uso del dinamómetro como herramienta para mejorar la fuerza de la mano en el adulto mayor. El artículo se desarrolla con un diseño documental basado en el modelo PRISMA, donde se han revisado artículos en inglés y español de los años del 2013- 2022, las bases científicas utilizadas fueron Embase, Redalyc, LILACS, SciELO, Springer, Elsevier, EBSCO, Nature Geriatric, Science Proyect y Google Académico. Luego de la revisión se concluyó que el uso del dinamómetro es de fácil acceso para los adultos mayores y que mejora la fuerza de agarre en la mano, mejorando su calidad de vida.\&nbsp;},
	language = {es},
	number = {6},
	journal = {Revista Científica Arbitrada Multidisciplinaria PENTACIENCIAS},
	author = {Ortega, Mayra Lasteña Millingalli and Zamora, María Narciza Cedeño and Córdova, Grace Verónica Moscoso and Vega, Mónica del Rocío Caiza},
	month = oct,
	year = {2023},
	keywords = {adulto mayor, destreza motora, dinamómetro, funcionalidad, mano},
	pages = {369--383},
	file = {Full Text PDF:C\:\\Users\\34644\\Zotero\\storage\\AVJSQMDV\\Ortega et al. - 2023 - Uso del dinamómetro para mejorar la fuerza de la mano del adulto mayor Revisión bibliográfica.pdf:application/pdf},
